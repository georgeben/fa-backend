    "lint": "node_modules/.bin/eslint \"./src/**/*.js\"",
    "docs:api": "echo 'Building REST Docs...' && apidoc -i src/interfaces/http/routes/ -o docs/apidocs"
  },
  "author": "Kurobara George",
  "dependencies": {
    "apidoc": "^0.29.0",
    "awilix": "^4.3.3",
    "awilix-express": "^4.0.0",
    "body-parser": "^1.19.0",
    "convict": "^6.0.1",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dayjs": "^1.10.7",
    "dotenv-extended": "^2.9.0",
    "express": "^4.17.1",
    "express-joi-validation": "^5.0.0",
    "google-auth-library": "^7.12.0",
    "helmet": "^4.5.0",
    "http-status-codes": "^2.1.4",
    "joi": "^17.4.1",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "mongoose": "^6.0.11",
    "morgan": "^1.10.0",
