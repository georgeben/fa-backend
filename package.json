    "@babel/register": "^7.13.16",
    "babel-plugin-module-resolver": "^4.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.25.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-babel-module": "^5.3.0",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^7.0.0",
    "nodemon": "^2.0.7",
    "nyc": "^15.1.0"
