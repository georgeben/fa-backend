{
  "name": "conference-api",
  "version": "1.0.0",
  "description": "API for Future Africa Conference",
  "main": "src/index.js",
  "scripts": {
    "build": "babel src --delete-dir-on-start --copy-files -d dist",
    "prestart": "npm run build",
    "start:dev": "nodemon \"src/index.js\"",
    "start": "NODE_ENV=production pm2-runtime start ecosystem.config.js",
